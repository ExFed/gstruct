/*+++
  [expect]
  names = [
      'String',
      'Number',
      'Bool',
      'StringLike',
      'NumberLike',
      'BoolLike',
      'Data',
      'Value',
      'FakeNews',
      'Deferred',
      'LateBloomer'
  ]
*/

typedef String : primitive
typedef Number : primitive
typedef Bool : primitive

typedef StringLike : String // test collision w/ default import (java.lang.String)
typedef NumberLike : Number // test collision w/ default import (java.lang.Number)
typedef BoolLike : Bool

/* check string references */
typedef Data : 'String'


// verify specifier w/ bound ref
typedef Value : Number {
    description = 'valuable to somebody somewhere'
}

// verify specifier w/ string ref
typedef FakeNews : 'BoolLike' {
    description = 'consists of alternative facts'
}

// out-of-order definition
typedef Deferred : 'LateBloomer'
typedef LateBloomer : primitive
